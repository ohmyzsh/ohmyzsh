_swift_package_reset() {
    integer ret=1
    local -a args
    args+=(
        '--version[Show the version.]'
        '(-help -h --help)'{-help,-h,--help}'[Show help information.]'
    )
    _arguments -w -s -S $args[@] && ret=0

    return ret
