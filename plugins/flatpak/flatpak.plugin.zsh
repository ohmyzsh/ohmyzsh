# Flatpak aliases

alias fpv='flatpak --version'
alias fpf='flatpak search'
alias fpi='flatpak install'
alias fpiu='flatpak install --user'
alias fpin='flatpak info'
alias fpr='flatpak uninstall'
alias fpref='flatpak update'
alias fpl='flatpak list'
alias fpd='flatpak disable'
alias fpe='flatpak enable'
alias fprl='flatpak remote-list'
alias fpra='flatpak remote-add --if-not-exists'
