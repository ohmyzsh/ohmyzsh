function p {
  # The p function changes your ICON environment variable.
  # Instantly update your prompt by typing something like this:
  # p BEER_MUG
  NEWICON=$1
  EMOJI_KEYS=(${(k)EMOJI})
  if (($EMOJI_KEYS[(Ie)$NEWICON])); then
    export ICON=$1
  else
    echo "Emoji not found."
  fi
}

function showemoji {
  # Running 'showemoji' will give you the whole list of emoji with names
